{
  "layers": [

    {
      "id": "features_raw",
      "type": "line",
      "source": "feature-features_raw",
      "layout": {
        "line-join": "round",
        "line-cap": "round"
      },
      "paint": {
        "line-color": "red",
        "line-opacity": 1,
        "line-width": 3
      }
    },
    {
      "id": "cluster_grid",
      "type": "fill",
      "source": "cluster-cluster_grid",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": {
          "property": "point_count_normalize",
          "type": "exponential",
          "stops": [
            [
              0,
              "#36a996"
            ],
            [
              20,
              "#3683a9"
            ],
            [
              40,
              "#3649a9"
            ],
            [
              60,
              "#5c36a9"
            ],
            [
              80,
              "#9636a9"
            ],
            [
              100,
              "#a93683"
            ]
          ]
        },
        "fill-opacity": 0.7
      }
    },
    {
      "id": "cluster_heat",
      "type": "heatmap",
      "source": "cluster-cluster_heatmap",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "heatmap-weight": [
          "interpolate",
          [
            "linear"
          ],
          [
            "get",
            "point_count_normalize"
          ],
          0,
          0,
          100,
          1
        ],
        "heatmap-intensity": 3,
        "heatmap-color": [
          "interpolate",
          [
            "exponential",
            10
          ],
          [
            "heatmap-density"
          ],
          0,
          "rgba(33,102,172,0)",
          0.01,
          "rgb(33,102,172)",
          0.2,
          "rgb(103,169,207)",
          0.4,
          "rgb(209,229,240)",
          0.6,
          "rgb(253,219,199)",
          0.8,
          "rgb(239,138,98)",
          1,
          "rgb(178,24,43)"
        ],
        "heatmap-radius": 50,
        "heatmap-opacity": 0.6
      }
    }
  ]
}
