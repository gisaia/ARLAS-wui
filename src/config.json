      "idFieldName": "id",
      "refresh": false,
      "components": {
        "chipssearch": {
          "search_size": 100,
          "name": "Search a MMSI",
          "icon": "search",
          "autocomplete_field": "vessel.ship_type",
          "autocomplete_size": "20"
        },
        "geosort": {
          "dragRatio": 0.1,
          "minGeosortZoom": 8
        }
      }
    }
  }
