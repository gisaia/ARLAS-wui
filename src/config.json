{
    "catalog": {
        "web": {
            "app": {
                "fieldsConfiguration": {
                    "idFieldName": "id"
                },
                "components": {
                    "histogram$timeline": {
                        "aggregationmodel": {
                            "type": "datehistogram",
                            "field": "timestamp",
                            "interval": {
                                "value": "1",
                                "unit": "Week"
                            }
                        },
                        "name": "Timeline",
                        "icon": "watch_later"
                    },
                    "histogram$velocity": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "velocity",
                            "interval": {
                                "value": "30"
                            }
                        },
                        "name": "Velocity",
                        "charttype": "bars",
                        "title": "Velocity"
                    },
                    "histogram$heading": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "heading",
                            "interval": {
                                "value": "10"
                            }
                        },
                        "name": "Heading",
                        "title": "Heading",
                        "charttype": "area"
                    },
                    "histogram$geoaltitude": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "geoaltitude",
                            "interval": {
                                "value": "600"
                            }
                        },
                        "name": "Geo-Altitutde",
                        "charttype": "bars",
                        "title": "Geo-altitude"

                    },
                    "histogram$velocityFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "velocity",
                            "interval": {
                                "value": "10"
                            }
                        },
                        "name": "Velocity",
                        "charttype": "bars",
                        "title": "Velocity",
                        "height": 120
                    },
                    "histogram$headingFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "heading",
                            "interval": {
                                "value": "15"
                            }
                        },
                        "name": "Heading",
                        "charttype": "area",
                        "title": "Heading",
                        "height": 120
                    },
                    "histogram$geoaltitudeFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "geoaltitude",
                            "interval": {
                                "value": "300"
                            }
                        },
                        "name": "Geo-altitude",
                        "charttype": "bars",
                        "title": "Geo-altitude",
                        "height": 120
                    },
                    "map$mapbox": {
                        "search_size": 100,
                        "name": "Map",
                        "icon": "check_box_outline_blank",
                        "geometry": "geometry",
                        "actions": {
                            "flyto": true,
                            "hightlight": true
                        },
                        "aggregationmodel": {
                            "type": "geohash",
                            "field": "centroid",
                            "interval": {
                                "value": "1"
                            },
                            "withGeoCentroid": true
                        },
                        "basemapUrl": "http://osm-liberty.lukasmartinelli.ch/style.json",
                        "fontClusterlabel": "Roboto Regular",
                        "zoomLevelFullData": 1,
                        "zoomLevelForTestCount": 6,
                        "nbMaxFeatureForCluster": 1000,
                        "margeExtendPctForLoad": 250,
                        "margeExtendPctForPan": 25,
                        "zoomToNbMaxFeatureForCluster": {
                            "4": 2000,
                            "5": 2000,
                            "6": 1000,
                            "7": 600,
                            "8": 500,
                            "9": 200,
                            "10": 200,
                            "11": 200,
                            "12": 50
                        },
                        "paintRuleGeoBox": {
                            "line-color": "#FC9F28",
                            "line-opacity": 0.2
                        },
                        "paintRuleCLusterFeatureFill": {
                            "fill-color": "#56A7EE",
                            "fill-opacity": 0.5
                        },
                        "paintRuleClusterFeatureLine": {
                            "line-color": "#56A7EE",
                            "line-opacity": 1,
                            "line-width": 3
                        },
                        "paintRuleClusterCircle": {
                            "circle-stroke-width": 2,
                            "circle-stroke-color": "#D3D3D3",
                            "circle-color": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        "#f7fcf0"
                                    ],
                                    [
                                        20,
                                        "#e0f3db"
                                    ],
                                    [
                                        30,
                                        "#ccebc5"
                                    ],
                                    [
                                        40,
                                        "#a8ddb5"
                                    ],
                                    [
                                        50,
                                        "#7bccc4"
                                    ],
                                    [
                                        60,
                                        "#4eb3d3"
                                    ],
                                    [
                                        70,
                                        "#2b8cbe"
                                    ],
                                    [
                                        90,
                                        "#3690c0"
                                    ]
                                ]
                            },
                            "circle-radius": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        22
                                    ],
                                    [
                                        20,
                                        24
                                    ],
                                    [
                                        30,
                                        26
                                    ],
                                    [
                                        40,
                                        28
                                    ],
                                    [
                                        50,
                                        30
                                    ],
                                    [
                                        60,
                                        32
                                    ],
                                    [
                                        70,
                                        34
                                    ],
                                    [
                                        90,
                                        36
                                    ]
                                ]
                            },
                            "circle-opacity": 1
                        }
                    },
                    "countall$countall": {
                        "search_size": 100,
                        "name": "CountAll"
                    },
                    "chipssearch$chipssearch": {
                        "search_size": 100,
                        "name": "Search",
                        "icon": "search"
                    }
                }
            }
        }
    },
    "server": {
        "server$default": {
            "url": "http://localhost:9999/arlas"
        },
        "collection$default": {
            "collection": "opensky"
        }
    }
}
