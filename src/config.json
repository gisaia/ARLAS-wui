{
    "catalog": {
        "web": {
            "app": {
                "fieldsConfiguration": {
                    "idFieldName": "citationIdentifier"
                },
                "components": {
                    "histogram$timeline": {
                        "aggregationmodel": {
                            "type": "datehistogram",
                            "field": "imageExtension.centerViewingDateTime",
                            "interval": {
                                "value": "1",
                                "unit": "Week"
                            }
                        },
                        "name": "Timeline",
                        "icon": "watch_later"
                    },
                    "histogram$cloud": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.cloudCoverPercentage",
                            "interval": {
                                "value": "15"
                            }
                        },
                        "name": "Cloud %",
                        "charttype": "bars",
                        "title": "Cloud coverage :"
                    },
                    "histogram$quality": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.incidenceAngle",
                            "interval": {
                                "value": "3"
                            }
                        },
                        "name": "Incidence %",
                        "charttype": "onedimension",
                        "title": "Incidence :",
                        "height": 5
                    },
                    "histogram$azimut": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.illuminationAzimuthAngle",
                            "interval": {
                                "value": "6"
                            }
                        },
                        "name": "Azimut",
                        "charttype": "onedimension",
                        "title": "Azimut :",
                        "height": 5
                    },
                    "histogram$cloudFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.cloudCoverPercentage",
                            "interval": {
                                "value": "3"
                            }
                        },
                        "name": "Cloud %",
                        "dateunit": "millisecond",
                        "charttype": "bars",
                        "title": "Cloud coverage :",
                        "height": 120
                    },
                    "histogram$qualityFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.incidenceAngle",
                            "interval": {
                                "value": "3"
                            }
                        },
                        "name": "Incidence %",
                        "dateunit": "millisecond",
                        "charttype": "onedimension",
                        "title": "Incidence :",
                        "height": 5
                    },
                    "histogram$azimutFilter": {
                        "aggregationmodel": {
                            "type": "histogram",
                            "field": "imageExtension.illuminationAzimuthAngle",
                            "interval": {
                                "value": "3"
                            }
                        },
                        "name": "Azimut",
                        "dateunit": "millisecond",
                        "charttype": "onedimension",
                        "title": "Azimut :",
                        "height": 5
                    },
                    "map$mapbox": {
                        "search_size": 100,
                        "name": "Map",
                        "icon": "check_box_outline_blank",
                        "geometry": "geographicBoundingPolygon.geometry",
                        "actions": {
                            "flyto": true,
                            "hightlight": true
                        },
                        "aggregationmodel": {
                            "type": "geohash",
                            "field": "imageExtension.centerPoint.geometry.coordinates",
                            "interval": {
                                "value": "2"
                            },
                            "withGeoCentroid": true
                        },
                        "basemapUrl": "http://osm-liberty.lukasmartinelli.ch/style.json",
                        "fontClusterlabel": "Roboto Regular",
                        "zoomLevelFullData": 1,
                        "zoomLevelForTestCount": 6,
                        "nbMaxFeatureForCluster": 1000,
                        "margeExtendPctForLoad": 250,
                        "margeExtendPctForPan": 25,
                        "zoomToNbMaxFeatureForCluster": {
                            "4": 2000,
                            "5": 2000,
                            "6": 1000,
                            "7": 600,
                            "8": 500,
                            "9": 200,
                            "10": 200,
                            "11": 200,
                            "12": 50
                        },
                        "paintRuleGeoBox": {
                            "line-color": "#FC9F28",
                            "line-opacity": 0.2
                        },
                        "paintRuleCLusterFeatureFill": {
                            "fill-color": "#56A7EE",
                            "fill-opacity": 0.5
                        },
                        "paintRuleClusterFeatureLine": {
                            "line-color": "#56A7EE",
                            "line-opacity": 1,
                            "line-width": 3
                        },
                        "paintRuleClusterCircle": {
                            "circle-stroke-width": 2,
                            "circle-stroke-color": "#D3D3D3",
                            "circle-color": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        "#f7fcf0"
                                    ],
                                    [
                                        20,
                                        "#e0f3db"
                                    ],
                                    [
                                        30,
                                        "#ccebc5"
                                    ],
                                    [
                                        40,
                                        "#a8ddb5"
                                    ],
                                    [
                                        50,
                                        "#7bccc4"
                                    ],
                                    [
                                        60,
                                        "#4eb3d3"
                                    ],
                                    [
                                        70,
                                        "#2b8cbe"
                                    ],
                                    [
                                        90,
                                        "#3690c0"
                                    ]
                                ]
                            },
                            "circle-radius": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        22
                                    ],
                                    [
                                        20,
                                        24
                                    ],
                                    [
                                        30,
                                        26
                                    ],
                                    [
                                        40,
                                        28
                                    ],
                                    [
                                        50,
                                        30
                                    ],
                                    [
                                        60,
                                        32
                                    ],
                                    [
                                        70,
                                        34
                                    ],
                                    [
                                        90,
                                        36
                                    ]
                                ]
                            },
                            "circle-opacity": 1
                        }
                    },
                    "countall$countall": {
                        "search_size": 100,
                        "name": "CountAll"
                    },
                    "chipssearch$chipssearch": {
                        "search_size": 100,
                        "name": "Search",
                        "icon": "search"
                    }
                }
            }
        }
    },
    "server": {
        "server$default": {
            "url": "http://localhost:9999/arlas"
        },
        "collection$default": {
            "collection": "level0_enriched"
        }
    }
}