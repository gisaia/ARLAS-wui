{
  "name": "arlas-wui",
  "version": "24.1.0-dev",
  "author": "gisaia",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package --patch-dir node_modules/arlas-web-core/patches && patch-package --patch-dir node_modules/arlas-d3/patches",
    "start": "ng serve",
    "build": "ng build --configuration production --aot --base-href='$ARLAS_WUI_BASE_HREF/'",
    "build-lib": "ng build arlas-wui-lib",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --format json"
  },
  "peerDependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "rxjs": "~7.4.0",
    "zone.js": "^0.11.4"
  },
  "dependencies": {
    "@ngx-translate/core": "14.0.0",
    "arlas-wui-toolkit": "~24.0.1",
    "hammerjs": "^2.0.8",
    "ng-packagr": "^14.2.2",
    "ngx-markdown": "14.0.1",
    "patch-package": "^6.4.7",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@gisaia/ngx-translate-extract": "8.2.0",
    "@ngneat/spectator": "~9.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "16.11.20",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.6.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-mocks": "~14.10.1",
    "node": "^16.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.6.0"
  },
  "overrides": {
    "portable-fetch": {
      "node-fetch": "3.3.0"
    },
    "shpjs": "4.0.4"
  }
}
